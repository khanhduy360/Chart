<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#0d09de">
        <ImageButton
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginTop="10dp"
            android:layout_marginStart="5dp"
            android:id="@+id/imagebuttoncot"
            android:src="@mipmap/back1"
            android:background="#0d09de"
            android:contentDescription="@string/text8" />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/txt21"
            android:textSize="20sp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            android:textColor="#ffffff"
            android:textStyle="bold"/>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/ln1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:orientation="horizontal">
        <ImageView
            android:contentDescription="@string/text8"
            android:layout_marginLeft="5dp"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginRight="5dp"
            android:background="#ffffbb33"
            android:layout_marginTop="2dp"
            android:layout_marginBottom="2dp" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="@string/text8"
            android:layout_marginTop="2dp"
            android:textSize="15sp"
            android:textStyle="italic"
            android:textColor="#da0909"
            android:id="@+id/textviewtenduong1"/>

        <ImageView
            android:contentDescription="@string/text8"
            android:layout_marginStart="5dp"
            android:layout_width="20dp"
            android:layout_marginEnd="5dp"
            android:layout_height="20dp"
            android:background="#ff33b5e5"
            android:layout_marginTop="2dp"
            android:layout_marginBottom="2dp"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="@string/text8"
            android:layout_marginStart="5dp"
            android:layout_marginTop="2dp"
            android:textStyle="italic"
            android:textSize="15sp"
            android:textColor="#da0909"
            android:id="@+id/textviewtenduong2"/>

        <ImageView
            android:contentDescription="@string/text8"
            android:layout_marginStart="2dp"
            android:layout_width="20dp"
            android:layout_marginEnd="5dp"
            android:layout_height="20dp"
            android:background="#ff99cc00"
            android:layout_marginTop="2dp"
            android:layout_marginBottom="2dp"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="@string/text8"
            android:layout_marginStart="5dp"
            android:layout_marginTop="2dp"
            android:textStyle="italic"
            android:textSize="15sp"
            android:textColor="#da0909"
            android:id="@+id/textviewtenduong3"/>
        <ImageView
            android:contentDescription="@string/text8"
            android:layout_marginStart="2dp"
            android:layout_width="20dp"
            android:layout_marginEnd="5dp"
            android:layout_height="20dp"
            android:background="#ff3300"
            android:layout_marginTop="2dp"
            android:layout_marginBottom="2dp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="@string/text8"
            android:layout_marginStart="5dp"
            android:layout_marginTop="2dp"
            android:textStyle="italic"
            android:textSize="15sp"
            android:textColor="#da0909"
            android:id="@+id/textviewtenduong4"/>
        <ImageView
            android:contentDescription="@string/text8"
            android:layout_marginStart="2dp"
            android:layout_width="20dp"
            android:layout_marginEnd="5dp"
            android:layout_height="20dp"
            android:background="#FF8C00"
            android:layout_marginTop="2dp"
            android:layout_marginBottom="2dp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="@string/text8"
            android:layout_marginStart="5dp"
            android:layout_marginTop="2dp"
            android:textStyle="italic"
            android:textSize="15sp"
            android:textColor="#da0909"
            android:id="@+id/textviewtenduong5"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp">
        <ImageView
            android:contentDescription="@string/text8"
            android:layout_marginLeft="5dp"
            android:layout_width="20dp"
            android:layout_marginRight="5dp"
            android:layout_height="20dp"
            android:background="#87CEFA"
            android:layout_marginTop="2dp"
            android:layout_marginBottom="2dp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="@string/text8"
            android:layout_marginStart="5dp"
            android:layout_marginTop="2dp"
            android:textStyle="italic"
            android:textSize="15sp"
            android:textColor="#da0909"
            android:id="@+id/textviewtenduong6"/>
    </LinearLayout>
    <com.github.mikephil.charting.charts.BarChart
        android:id="@+id/BarChartMpAndroid"
        android:layout_width="400dp"
        android:layout_height="400dp"
        android:layout_gravity="center"
        android:layout_marginTop="20dp">
    </com.github.mikephil.charting.charts.BarChart>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/texViewBar"
        android:textColor="#da0909"
        android:textStyle="italic"
        android:textAlignment="center"
        android:textSize="15sp"/>
</LinearLayout>